{
    "name": "tben/omnipay-yoomoney",
    "type": "library",
    "description": "Yoomoney gateway for Omnipay payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "yoomoney",
        "yookassa",
        "purchase"
    ],
    "homepage": "https://github.com/tben/omnipay-yoomoney",
    "license": "MIT",
    "authors": [
        {
            "name": "Ben Tidy",
            "email": "yoomoney@bentidy.me"
        }
    ],
    "autoload": {
        "psr-4": {
            "Omnipay\\YooMoney\\": "src",
            "Omnipay\\YooMoney\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.1",
        "omnipay/common": "^3.2.0"
    },
    "require-dev": {
        "omnipay/tests": "^4.1.2",
        "squizlabs/php_codesniffer": "^3"
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    },
    "prefer-stable": true
}
